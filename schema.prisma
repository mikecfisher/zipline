generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Product {
  id           Int    @id @default(autoincrement())
  mass_g       Float
  product_name String
  product_id   Int
}

model Order {
  id         Int @id @default(autoincrement())
  order_id   Int
  product_id Int
  quantity   Int
}
